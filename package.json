    "rimraf": "^2.6.3",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {
    "@babel/cli": "^7.2.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.3"
  },
