{
  "name": "react-djforms",
  "version": "0.2.0",
  "description": "DJForms is a simple way for building forms to your react app using a flexible structure.",
  "author": "Luann Francisco",
  "keywords": [],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dudow8/react-djforms.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
